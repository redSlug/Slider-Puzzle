{


  "env": {
        "es6": true
    },

  "plugins": [
    "react"
  ],

  "settings": {
    "react": {
      "createClass": "createClass", 
      "pragma": "React",  
      "version": "15.0" 
    }
  },

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
    "extends": "google",
    "installedESLint": true
}